    "print(\"   \")\n",
    "print(\"   ‚úÖ CORRECT way to combine:\")\n",
    "print(\"      Total population: 1,000 + 2,000 = 3,000 (SUM)\")\n",
    "print(\"      Poverty rate: (200 + 200) / 3,000 = 13.3% (WEIGHTED AVERAGE)\")\n",
    "print(\"   \")\n",
    "print(\"   ‚ùå WRONG way:\")\n",
    "print(\"      Poverty rate: (20% + 10%) / 2 = 15% (SIMPLE AVERAGE)\")\n",
